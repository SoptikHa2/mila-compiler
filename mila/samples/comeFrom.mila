program comingFrom;

var i: integer;
begin
    i := 0;
    :a;

    comeFrom a;
    writeln('First');
    writeln(i);
    i := 1;
    writeln(i);
    writeln('First end');
    exit;

    comeFrom a;
    writeln('Second');
    writeln(i);
    i := 2;
    writeln(i);
    writeln('Second end');
    exit;
end.
